      <pipeline>
        <process id="gstreamer">
        <![CDATA[
            (gst-construct-pipeline
                "audioconvert"
                (gst-construct-element "lame"
                    (if (= vbr_mode -1)
                        ("preset" preset)
                        ("mode" 4
                         (if (= vbr_mode 0)
                             ("bitrate" bitrate)
                             ("vbr" vbr_mode "vbr-quality" (- 9 vbr_quality))))))
                (if (and 
                    (!= vbr_mode 0) 
                    (gst-element-is-available "xingmux")) 
                        "xingmux" 
                        "")
                (if (gst-element-is-available "id3v2mux")
                    "id3v2mux"
                    "id3mux"))
        ]]>
        </process>
